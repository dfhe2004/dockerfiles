RUN apt-get update && apt-get install -y \
  libopenblas-dev \
  cython \
  wget \
  python-setuptools \
  python-scipy \
  python-sklearn \
  python-tables \
  python-matplotlib \
  python-opencv \
  python-numexpr \
  python-skimage \
  python-pil \
   > /dev/null